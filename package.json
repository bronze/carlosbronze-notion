{
  "name": "nextjs-notion-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps:upgrade": "[ -z $GITHUB_ACTIONS ] && pnpm up -L notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && run-s deps:link:* || echo 'Skipping deps:update on CI'",
    "deps:unlink": "[ -z $GITHUB_ACTIONS ] && pnpm add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link:notion-types": "pnpm link ../react-notion-x/packages/notion-types",
    "deps:link:notion-utils": "pnpm link ../react-notion-x/packages/notion-utils",
    "deps:link:notion-client": "pnpm link ../react-notion-x/packages/notion-client",
    "deps:link:react-notion-x": "pnpm link ../react-notion-x/packages/react-notion-x",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check --write"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.18.2",
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@icons-pack/react-simple-icons": "^13.4.0",
    "@keyvhq/core": "^2.1.11",
    "@keyvhq/redis": "^2.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-icons/all-files": "^4.1.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.4.1",
    "ky": "^1.8.1",
    "lqip-modern": "^2.2.1",
    "lucide-react": "^0.525.0",
    "next": "^15.3.4",
    "notion-client": "^7.7.1",
    "notion-types": "^7.7.1",
    "notion-utils": "^7.7.1",
    "p-map": "^7.0.3",
    "p-memoize": "^7.1.1",
    "postcss": "^8.5.6",
    "posthog-js": "^1.257.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^19.1.0",
    "react-notion-x": "^7.7.1",
    "react-tweet": "^3.2.2",
    "react-use": "^17.6.0",
    "rss": "^1.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@fisch0920/config": "^1.1.4",
    "@fisch0920/eslint-config": "^1.4.0",
    "@iconify/react": "^6.0.2",
    "@next/bundle-analyzer": "^15.3.4",
    "@next/eslint-plugin-next": "^15.3.4",
    "@types/node": "^24.0.8",
    "@types/react": "^19.1.8",
    "@types/react-body-classname": "^1.1.10",
    "@types/rss": "^0.0.32",
    "cross-env": "^7.0.2",
    "eslint": "^9.31.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  },
  "prettier": "@fisch0920/config/prettier",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --ignore-unknown --write",
      "eslint --fix"
    ]
  }
}
